[
  {
    "type": "select",
    "label": "Вид объекта",
    "propName": "objectType",
    "validation": {
      "required": "true"
    }
  },
  {
    "type": "input",
    "label": "Номер объекта",
    "propName": "objectNumber.full",
    "validation": {
      "regexp": "^([1-9][0-9]{9})([0-9]{2})([0-9]{6})$"
    }
  },
  {
    "type": "input-group",
    "options": {
      "direction": "horizontal"
    },
    "items": [
      {
        "type": "input",
        "propName": "objectNumber.detailed.soato",
        "validation": {
          "regexp": "^([1-9][0-9]{9})$"
        }
      },
      {
        "type": "input",
        "propName": "objectNumber.detailed.block",
        "validation": {
          "regexp": "^([0-9]{2})$"
        }
      },
      {
        "type": "input",
        "propName": "objectNumber.detailed.order",
        "validation": {
          "regexp": "^([0-9]{6})$"
        }
      }
    ]
  }
]